{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "chrome": "53"
                },
                "modules": false,
                "useBuiltIns": false
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": true,
                "regenerator": false,
                "useESModules": false
            }
        ]
    ]
}